.page-header
  %h1
    Equipment Reservation
    %small Change the equipment reservation.
%br
= form_for [:admin, equipment_reservation], html: { class: 'form-horizontal' } do |f|
  %fieldset
    .control-group
      = f.label :name
      .controls
        .input
          = f.text_field :name
    .control-group
      = f.label :from
      .controls
        .input
          = f.text_field :from
    .control-group
      = f.label :to
      .controls
        .input
          = f.text_field :to
    .form-actions
      %input.btn-primary{type: 'submit'}
